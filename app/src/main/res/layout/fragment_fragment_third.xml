<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.jeonjin_il.mysecondapp.Fragment_third">

    <TextView
        android:text="설정"
        android:textSize="30dp"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="143dp"
        android:layout_marginStart="143dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp">

        <TextView
            android:layout_width="match_parent"
            android:text="-------------------------------------------------------------------------------------"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:text="서비스 시작시간 : "
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                />
            <TextView
                android:text=" 08:00 "
                android:layout_weight="1"
                android:id="@+id/third_start_text"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                />
            <Button
                android:id="@+id/third_start_time_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="onButtonClicked"
                android:text="시간 설정하기"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:text="서비스 종료시간 : "
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                />
            <TextView
                android:text="20:00"
                android:layout_weight="1"
                android:id="@+id/third_end_text"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                />
            <Button
                android:id="@+id/third_end_time_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="onButtonClicked"

                android:text="시간 설정하기"
                />
        </LinearLayout>
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/second_text" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:text="숫자"
            android:ems="10"
            android:id="@+id/second_text2" />

        <Button
            android:text="Button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/second_button" />
    </LinearLayout>




</RelativeLayout>
